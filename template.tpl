const log = require('logToConsole');
const injectScript = require('injectScript');
const makeTableMap = require('makeTableMap');
const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');
const createQueue = require('createQueue');
const createArgumentsQueue = require('createArgumentsQueue');
const queryPermission = require('queryPermission');

const ym = createArgumentsQueue('ym','ym.a');
const domains = data.trustedDomains ? data.domains : [];
let xparams = '';
if (data.gprice) {
  if (data.sparams) {
    xparams = makeTableMap(data.sparams, 'sparams_key', 'sparams_val');
    xparams.order_price = data.gprice;
    xparams.currency = data.gcurrency;
  } else {
    xparams = {};
    xparams.order_price = data.gprice;
    xparams.currency = data.gcurrency;
  }
} else {
  xparams = data.sparams ? makeTableMap(data.sparams, 'sparams_key', 'sparams_val') : undefined;
}
const sparams = xparams;
const uparams = data.uparams ? makeTableMap(data.uparams, 'uparams_key', 'uparams_val') : undefined;
const metodo = data.method;

if (metodo == "init") {
  let jsmetrica = 'https://mc.yandex.ru/metrika/tag.js';
  if(data.cdn === true) {
    jsmetrica = 'https://cdn.jsdelivr.net/npm/yandex-metrica-watch/tag.js';
  }
  injectScript(jsmetrica);
  if (data.ecommerce === true) {
    ym(data.counter,'init',{
      defer: data.defer,
      ut: data.ut,
      clickmap: data.clickmap,
      webvisor: data.webvisor,
      trackLinks: data.links,
      trackHash: data.hash,
      accurateTrackBounce: data.accurate,
      params: sparams,
      userParams: uparams,
      childIframe: data.childIframe,
      triggerEvent: data.triggerEvent,
      trustedDomains: domains,
      ecommerce: data.datacollection
    });
  } else {
    ym(data.counter,'init',{
      defer: data.defer,
      ut: data.ut,
      clickmap: data.clickmap,
      webvisor: data.webvisor,
      trackLinks: data.links,
      trackHash: data.hash,
      accurateTrackBounce: data.accurate,
      params: sparams,
      userParams: uparams,
      childIframe: data.childIframe,
      triggerEvent: data.triggerEvent,
      trustedDomains: domains
    });
  }
  if (data.notbounce === true) {
    ym(data.counter, 'notBounce');
  }
  if (data.extensions) {
    ym(data.counter, 'addFileExtension', data.extensions);
  }
  if (data.debug === true) {
    let cookieName = 'gtm_debug';
    let cookieValues;
    if (queryPermission('get_cookies', cookieName)) {
      cookieValues = getCookieValues(cookieName);
    }
    if (cookieValues === undefined) {
      setCookie('_ym_debug','0',{
        'domain': 'auto',
        'path': '/',
  		'max-age': 'Thu, 01 Jan 1970 00:00:01 GMT'
	  });
    } else {
      setCookie('_ym_debug','1',{
        'domain': 'auto',
        'path': '/',
  		'max-age': 60*60*24*365
	  });
    }
  }
} else if (metodo == "hit") {
  let options = {};
  if (data.hit_referer) {
    options.referer = data.hit_referer;
  }
  if (data.hit_title) {
    options.title = data.hit_title;
  }
  if (sparams) {
    options.params = sparams;
  }
  ym(data.counter, 'hit', data.hit_url, options);
} else if (metodo == "reachGoal") {
  ym(data.counter, 'reachGoal', data.target, sparams);
} else if (metodo == "extLink") {
  let options = {};
  if (data.extlink_title) {
    options.title = data.extlink_title;
  }
  if (sparams) {
    options.params = sparams;
  }
  ym(data.counter, 'extLink', data.extlink_url, options);
} else if (metodo == "file") {
  let options = {};
  if (data.file_referer) {
    options.referer = data.file_referer;
  }
  if (data.file_title) {
    options.title = data.file_title;
  }
  if (sparams) {
    options.params = sparams;
  }
  ym(data.counter, 'file', data.file_url, options);
} else if (metodo == "params") {
  let sessionParams = makeTableMap(data.sessionparams, 'sessionparams_key', 'sessionparams_val');
  if (data.gprice) {
    let goalParams = {};
    goalParams.order_price = data.gprice;
    goalParams.currency = data.gcurrency;
    ym(data.counter, 'params', sessionParams, goalParams);
  } else {
    ym(data.counter, 'params', sessionParams);
  }
} else if (metodo == "setUserID") {
  ym(data.counter, 'setUserID', data.userid);
} else if (metodo == "userParams") {
  let userParams = makeTableMap(data.userparams, 'userparams_key', 'userparams_val');
  ym(data.counter, 'userParams', userParams);
}

data.gtmOnSuccess();


